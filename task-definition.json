{
  "containerDefinitions": [
    {
      "name": "gptrouter",
      "image": "063003812447.dkr.ecr.ap-northeast-2.amazonaws.com/gptrouter:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "gptrouter-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        },
        {
          "name": "gptrouter-8000-tcp",
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/ChatGptApiRouter",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "ChatGptApiRouter",
  "taskRoleArn": "arn:aws:iam::063003812447:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::063003812447:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
